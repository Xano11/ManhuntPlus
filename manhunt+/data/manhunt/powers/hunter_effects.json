    "xolotl_tp": {
        "type": "origins:active_self",
        "entity_action": {
            "type": "origins:raycast",
            "distance": 152,
            "block": false,
            "entity": true,
            "shape_type": "visual",
            "fluid_handling": "none",
            "bientity_action": {
                "type": "origins:and",
                "actions": [
                    {
                        "type": "origins:if_else",
                        "condition": {
                            "type": "origins:actor_condition",
                            "condition": {
                                "type": "origins:resource",
                                "resource": "manhunt:hunter_effects_tp_times",
                                "comparison": "==",
                                "compare_to": 2
                            }
                        },
                        "if_action": {
                            "type": "origins:and",
                            "actions": [
                                {
                                    "type": "origins:invert",
                                    "action": {
                                        "type": "origins:add_to_set",
                                        "set": "manhunt:xolotl/wolf/wolves_offered"
                                    }
                                },
                                {
                                    "type": "origins:target_action",
                                    "action": {
                                        "type": "origins:and",
                                        "actions": [
                                            {
                                                "type": "origins:change_resource",
                                                "resource": "manhunt:xolotl/wolf/wolves_life",
                                                "change": 20,
                                                "operation": "set"
                                            },
                                            {
                                                "type": "origins:change_resource",
                                                "resource": "manhunt:xolotl/wolf/wolves_howling",
                                                "change": 1,
                                                "operation": "set"
                                            }
                                        ]
                                    }
                                },
                                {
                                    "type": "origins:actor_action",
                                    "action": {
                                        "type": "origins:and",
                                        "actions": [
                                            {
                                                "type": "origins:change_resource",
                                                "resource": "manhunt:hunter_effects_tp_times",
                                                "change": 3,
                                                "operation": "set"
                                            },
                                            {
                                                "type": "origins:execute_command",
                                                "command": "playsound minecraft:entity.generic.eat player @a ~ ~ ~ 1 0.5"
                                            }
                                        ]
                                    } 
                                }
                            ]
                        },
                        "else_action": {
                            "type": "origins:and",
                            "actions": [
                                {
                                    "type": "origins:actor_action",
                                    "action": {
                                        "type": "origins:and",
                                        "actions": [
                                            {
                                                "type": "origins:change_resource",
                                                "resource": "manhunt:hunter_effects_tp_times",
                                                "change": 1
                                            },
                                            {
                                                "type": "origins:change_resource",
                                                "resource": "manhunt:hunter_effects_tp_reset",
                                                "change": 30,
                                                "operation": "set"
                                            },
                                            {
                                                "type": "origins:execute_command",
                                                "command": "playsound minecraft:block.note_block.bell player @s ~ ~ ~ 1 1.4"
                                            }
                                        ]
                                    }
                                }
                            ]
                        }
                    }
                ]
            },
            "bientity_condition": {
                "type": "origins:target_condition",
                "condition": {
                    "type": "origins:and",
                    "conditions": [
                        {
                            "type": "origins:living"
                        },
                        {
                            "type": "origins:power",
                            "power": "manhunt:xolotl/wolf/wolves"
                        },
                        {
                            "type": "origins:resource",
                            "resource": "manhunt:xolotl/wolf/wolves_offering",
                            "comparison": "==",
                            "compare_to": 1
                        },
                        {
                            "type": "origins:set_size",
                            "set": "manhunt:xolotl/wolf/wolves_offered",
                            "comparison": "==",
                            "compare_to": 0
                        }
                    ]
                }
            },
            "command_step": 1
        },
        "cooldown": 1,
        "hud_render": {
            "should_render": false
        },
        "key": "key.use"
    },
