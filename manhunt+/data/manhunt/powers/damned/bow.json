{
    "type": "origins:multiple",
    "scoreboard": {
        "type": "origins:action_on_callback",
        "entity_action_gained": {
            "type": "origins:execute_command",
            "command": "scoreboard objectives add bow minecraft.used:minecraft.bow"
        },
        "execute_chosen_when_orb": true,
        "hidden": true
    },
    "tp": {
        "type": "origins:action_over_time",
        "entity_action": {
            "type": "origins:and",
            "actions": [
                {
                    "type": "origins:execute_command",
                    "command": "execute if entity @s[scores={bow=1..}] run tag @e[type=arrow] remove tp_skele"
                },
                {
                    "type": "origins:execute_command",
                    "command": "execute if entity @s[scores={bow=1..}] run tag @e[limit=1,sort=nearest,type=arrow] add tp_skele"
                },
                {
                    "type": "origins:change_resource",
                    "resource": "manhunt:bow_resource",
                    "change": 600
                },
                {
                    "type": "origins:delay",
                    "ticks": 1,
                    "action": {
                        "type": "origins:execute_command",
                        "command": "execute if entity @s[scores={bow=1..}] run scoreboard players set @s bow 0"
                    }
                }
            ]
        },
        "interval": 1
    },
    "resource": {
        "type": "origins:resource",
        "min": 0,
        "max": 600,
        "start_value": 0,
        "hud_render": {
            "should_render": false
        },
        "min_action": {
            "type": "origins:and",
            "actions": [
                {
                    "type": "origins:execute_command",
                    "command": "tag @e[type=arrow] remove tp_skele",
                    "permission_level": 4
                }
            ]
        }
    },
    "remove_resource": {
        "type": "origins:action_over_time",
        "entity_action": {
            "type": "origins:change_resource",
            "resource": "manhunt:bow_resource",
            "change": -10
        },
        "interval": 10
    },
    "particles": {
        "type": "origins:action_over_time",
        "entity_action": {
            "type": "origins:execute_command",
            "command": "execute at @e[limit=1,sort=nearest,type=arrow,tag=tp_skele] run particle minecraft:dust_color_transition 1 1 1 1 0.4 0 1 ~ ~ ~ 0.3 0.3 0.3 1 100",
            "permission_level": 4
        },
        "interval": 30
    },
    "particles2": {
        "type": "origins:action_over_time",
        "entity_action": {
            "type": "origins:execute_command",
            "command": "execute at @e[limit=1,sort=nearest,type=arrow,tag=tp_skele] run particle minecraft:witch ~ ~ ~ 0.1 0.1 0.1 1 20",
            "permission_level": 4
        },
        "interval": 30
    },
    "sound": {
        "type": "origins:action_over_time",
        "entity_action": {
            "type": "origins:execute_command",
            "command": "execute at @e[limit=1,sort=nearest,type=arrow,tag=tp_skele] run playsound minecraft:block.amethyst_cluster.break player @a[distance=..12] ~ ~ ~ 0.3 0.5 0.1",
            "permission_level": 4
        },
        "interval": 30
    },
    "activate": {
        "type": "origins:active_self",
        "entity_action": {
            "type": "origins:and",
            "actions": [
                {
                    "type": "origins:execute_command",
                    "command": "particle minecraft:item minecraft:bone ~ ~1 ~ 0.3 0.3 0.3 0.2 100"
                },
                {
                    "type": "origins:execute_command",
                    "command": "particle minecraft:dust 1 1 1 1 ~ ~1 ~ 0.5 0 0.5 0.2 100"
                },
                {
                    "type": "origins:execute_command",
                    "command": "particle minecraft:ash ~ ~1 ~ 0.5 0.7 0.5 0.2 300"
                },
                {
                    "type": "origins:execute_command",
                    "command": "playsound minecraft:entity.skeleton.converted_to_stray player @s ~ ~ ~ 1 0.5 1"
                },
                {
                    "type": "origins:delay",
                    "ticks": 1,
                    "action": {
                        "type": "origins:and",
                        "actions": [
                            {
                                "type": "origins:execute_command",
                                "command": "tp @s @e[limit=1,sort=nearest,type=arrow,tag=tp_skele]"
                            }
                        ]
                    }
                },
                {
                    "type": "origins:delay",
                    "ticks": 2,
                    "action": {
                        "type": "origins:and",
                        "actions": [
                            {
                                "type": "origins:execute_command",
                                "command": "particle minecraft:item minecraft:bone ~ ~1 ~ 0.3 0.3 0.3 0.2 100"
                            },
                            {
                                "type": "origins:execute_command",
                                "command": "particle minecraft:dust 1 1 1 1 ~ ~1 ~ 0.5 0 0.5 0.2 100"
                            },
                            {
                                "type": "origins:execute_command",
                                "command": "particle minecraft:ash ~ ~1 ~ 0.5 0.7 0.5 0.2 300"
                            },
                            {
                                "type": "origins:execute_command",
                                "command": "playsound minecraft:entity.skeleton.converted_to_stray player @s ~ ~ ~ 1 0.5 1"
                            },
                            {
                                "type": "origins:execute_command",
                                "command": "tag @e[limit=1,sort=nearest,type=arrow,tag=tp_skele] remove tp_skele"
                            },
                            {
                                "type": "origins:execute_command",
                                "command": "effect give @s minecraft:slowness 5 3"
                            }
                        ]
                    }
                }
            ]
        },
        "key": {
            "key": "key.origins.primary_active"
        },
        "cooldown": 9000,
        "hud_render": {
            "sprite_location": "origins:textures/gui/community/huang/resource_bar_01.png",
            "bar_index": 5,
            "should_render": true
        }
    },
    "tag":{
        "type": "origins:target_action_on_hit",
        "entity_action": {
            "type": "origins:execute_command",
            "command": "power grant @e[limit=1,sort=nearest] manhunt:tag_effects",
            "permission_level": 4
        },
        "damage_condition": {
            "type": "origins:projectile",
            "projectile": "minecraft:arrow"
        },
        "cooldown": 1,
        "hud_render": {
            "should_render": false,
            "bar_index": 0
        }
    },
    "speed_gain":{
        "type": "origins:self_action_on_hit",
        "entity_action": {
            "type": "origins:execute_command",
            "command": "power grant @s manhunt:bow_speed",
            "permission_level": 4
        },
        "damage_condition": {
            "type": "origins:projectile",
            "projectile": "minecraft:arrow"
        },
        "cooldown": 1,
        "hud_render": {
            "should_render": false,
            "bar_index": 0
        }
    },
    "name": "§l§nBone Warping",
    "description": "Using your primary ability key, you can teleport yourself to the most recent arrow you shot. Arrows remain tagged for 30 seconds, so don't wait too long!"
}