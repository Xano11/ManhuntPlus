{
    "type": "origins:multiple",
    "defence": {
        "type": "origins:modify_damage_taken",
        "modifier": {
            "name": "boat bunker",
            "operation": "multiply_total",
            "value": -0.5
        },
        "condition": {
            "type": "origins:riding",
            "bientity_condition": {
                "type": "origins:target_condition",
                "condition": {
                    "type": "origins:entity_type",
                    "entity_type": "minecraft:boat"
                }
            }
        }
    },
    "boost": {
        "type": "origins:resource",
        "min": 0,
        "max": 6,
        "start_value": 0,
        "hud_render": {
            "should_render": false
        }
    },
    "boost_down": {
        "type": "origins:action_over_time",
        "entity_action": {
            "type": "origins:change_resource",
            "resource": "manhunt:captain/bunker_boost",
            "change": -1
        },
        "interval": 1
    },
    "momentum": {
        "type": "origins:action_over_time",
        "entity_action": {
            "type": "origins:and",
            "actions": [
                {
                    "type": "origins:riding_action",
                    "action": {
                        "type": "origins:add_velocity",
                        "z": 2.0,
                        "space": "local_horizontal",
                        "set": false
                    },
                    "bientity_condition": {
                        "type": "origins:target_condition",
                        "condition": {
                            "type": "origins:and",
                            "conditions": [
                                {
                                    "type": "origins:entity_type",
                                    "entity_type": "minecraft:boat"
                                },
                                {
                                    "type": "origins:fluid_height",
                                    "fluid": "minecraft:water",
                                    "comparison": ">",
                                    "compare_to": 0
                                }
                            ]
                        }
                    }
                },
                {
                    "type": "origins:execute_command",
                    "command": "particle minecraft:dust_color_transition 0 0.4 1 1 0 1 1 ~ ~0.4 ~ 0.5 0 0.5 1 30"
                },
                {
                    "type": "origins:execute_command",
                    "command": "particle minecraft:enchanted_hit ~ ~0.4 ~ 0.5 0 0.5 0.1 30"
                }
            ]
        },
        "interval": 1,
        "condition": {
            "type": "origins:resource",
            "resource": "manhunt:captain/bunker_boost",
            "compare_to": 0,
            "comparison": ">"
        }
    },
    "speedboat": {
        "type": "origins:active_self",
        "entity_action": {
            "type": "origins:and",
            "actions": [
                {
                    "type": "origins:change_resource",
                    "resource": "manhunt:captain/bunker_boost",
                    "change": 4
                },
                {
                    "type": "origins:execute_command",
                    "command": "playsound minecraft:item.trident.riptide_2 player @a ~ ~ ~ 3 1.4"
                }
            ]
        },
        "key": {
            "key": "key.origins.secondary_active",
            "continuous": false
        },
        "cooldown": 60,
        "hud_render": {
            "should_render": true,
            "sprite_location": "origins:textures/gui/community/huang/resource_bar_01.png",
            "bar_index": 10
        },
        "condition": {
            "type": "origins:riding",
            "bientity_condition": {
                "type": "origins:target_condition",
                "condition": {
                    "type": "origins:and",
                    "conditions": [
                        {
                            "type": "origins:entity_type",
                            "entity_type": "minecraft:boat"
                        },
                        {
                            "type": "origins:fluid_height",
                            "fluid": "minecraft:water",
                            "comparison": ">",
                            "compare_to": 0
                        }
                    ]
                }
            }
        }
    },
    "strider": {
        "type": "origins:action_on_entity_use",
        "bientity_action": {
            "type": "origins:target_action",
            "action": {
                "type": "origins:and",
                "actions": [
                    {
                        "type": "origins:spawn_entity",
                        "entity_type": "minecraft:boat",
                        "tag": "{Type:\"oak\",Tags:[\"striderboat\"]}",
                        "entity_action": {
                            "type": "origins:selector_action",
                            "selector": "@e[type=strider,limit=1,sort=nearest]",
