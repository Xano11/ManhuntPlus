                            {
                                "type": "origins:execute_command",
                                "command": "summon minecraft:armor_stand ~ ~ ~ {NoGravity:1b,Invulnerable:1b,Marker:1b,Invisible:1b,Tags:[\"lurk_slow\"]}"
                            },
                            {
