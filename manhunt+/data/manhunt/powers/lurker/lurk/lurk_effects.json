                        "resource": "manhunt:lurker/lurk/lurk_effects_laser",
                        "comparison": "==",
                        "compare_to": 0
                    },
                    "if_action": {
                        "type": "origins:execute_command",
                        "command": "playsound minecraft:entity.warden.sonic_charge player @a ~ ~ ~ 2 1.4"
                    }
                },
                {
                    "type": "origins:change_resource",
                    "resource": "manhunt:lurker/lurk/lurk_effects_laser",
