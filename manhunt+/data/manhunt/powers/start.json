    },
    "ready_key": {
        "type": "origins:active_self",
        "entity_action": {
            "type": "origins:and",
            "actions": [
                {
                    "type": "origins:if_else",
                    "condition": {
                        "type": "origins:command",
                        "command": "execute if entity @s[tag=!ready]",
                        "comparison": "==",
                        "compare_to": 1
                    },
                    "if_action": {
                        "type": "origins:and",
                        "actions": [
                            {
                                "type": "origins:execute_command",
                                "command": "tag @s add ready"
                            },
                            {
                                "type": "origins:change_resource",
                                "resource": "manhunt:start_ready",
                                "change": 1
                            }
                        ]
                    },
                    "else_action": {
                        "type": "origins:and",
                        "actions": [
                            {
                                "type": "origins:execute_command",
                                "command": "tag @s remove ready"
                            },
                            {
                                "type": "origins:change_resource",
                                "resource": "manhunt:start_ready",
                                "change": -1
                            }
                        ]
                    }
                },
                {
                    "type": "origins:execute_command",
                    "command": "playsound minecraft:block.note_block.pling player @s"
                }
            ]
        },
        "cooldown": 10,
        "key": {
            "key": "key.swapOffhand"
        },
        "hud_render": {
            "should_render": false
        },
        "condition": {
            "type": "origins:resource",
            "resource": "manhunt:start_timer",
            "comparison": "<=",
            "compare_to": 10
        }
    },
