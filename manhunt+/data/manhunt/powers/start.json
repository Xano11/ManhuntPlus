    "repick_key": {
        "type": "origins:active_self",
        "entity_action": {
            "type": "origins:and",
            "actions": [
                {
                    "type": "origins:execute_command",
                    "command": "origin gui @s"
                },
                {
                    "type": "origins:execute_command",
                    "command": "tag @s remove ready"
                },
                {
                    "type": "origins:change_resource",
                    "resource": "manhunt:start_ready",
                    "change": -1
                }
            ]
        },
        "cooldown": 10,
        "key": {
            "key": "key.jump"
        },
        "hud_render": {
            "should_render": false
