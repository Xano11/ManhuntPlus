                {
                    "type": "origins:if_else_list",
                    "actions": [
                        {
                            "condition": {
                                "type": "origins:resource",
                                "resource": "manhunt:setup_armorstand_countdown",
                                "comparison": "==",
                                "compare_to": 30
                            },
                            "action": {
                                "type": "origins:execute_command",
                                "command": "execute as @a run resource set @s manhunt:countdown_effects_timer 600"
                            }
                        },
                        {
                            "condition": {
                                "type": "origins:resource",
                                "resource": "manhunt:setup_armorstand_countdown",
                                "comparison": "==",
                                "compare_to": 15
                            },
                            "action": {
                                "type": "origins:execute_command",
                                "command": "execute as @a run resource set @s manhunt:countdown_effects_timer 300"
                            }
                        },
                        {
                            "condition": {
                                "type": "origins:resource",
                                "resource": "manhunt:setup_armorstand_countdown",
                                "comparison": "==",
                                "compare_to": 10
                            },
                            "action": {
                                "type": "origins:execute_command",
                                "command": "execute as @a run resource set @s manhunt:countdown_effects_timer 200"
                            }
                        }
                    ]
                },
                {
                    "type": "origins:execute_command",
                    "command": "function manhunt:countdown"
                },
                {
                    "type": "origins:execute_command",
                    "command": "power remove @a manhunt:start"
                },
                {
                    "type": "origins:execute_command",
                    "command": "power remove @s manhunt:setup_armorstand_ready_timer_tick"
                }
            ]
        }
    },
    "repick_timer_tick": {
        "type": "origins:action_over_time",
        "entity_action": {
            "type": "origins:and",
            "actions": [
                {
                    "type": "origins:change_resource",
                    "resource": "manhunt:setup_armorstand_repick_timer",
                    "change": -10
                }
            ]
        },
        "interval": 10,
        "condition": {
            "type": "origins:command",
            "command": "execute if entity @s[tag=repick]",
            "comparison": "==",
            "compare_to": 1
        }
    },
    "repick_timer_scoreboard": {
        "type": "origins:action_over_time",
        "entity_action": {
            "type": "origins:and",
            "actions": [
                {
                    "type": "origins:execute_command",
                    "command": "scoreboard players remove @s repick_timer 1"
                },
                {
                    "type": "origins:if_else",
                    "condition": {
                        "type": "origins:resource",
                        "resource": "manhunt:setup_armorstand_repick_timer",
                        "comparison": "<=",
                        "compare_to": 300
                    },
                    "if_action": {
                        "type": "origins:and",
                        "actions": [
                            {
                                "type": "origins:execute_command",
                                "command": "title @a title [\"\"]"
                            },
                            {
                                "type": "origins:execute_command",
                                "command": "title @a[tag=hunter] subtitle [\"\",\"[\",{\"score\":{\"name\":\"@s\",\"objective\":\"repick_timer\"},\"bold\":true},\"⏱]\"]"
                            },
                            {
                                "type": "origins:execute_command",
                                "command": "playsound minecraft:block.note_block.hat player @a[tag=hunter] ~ ~ ~ 3 1"
                            }
                        ]
                    }
                }
            ]
        },
        "interval": 20,
        "condition": {
            "type": "origins:command",
            "command": "execute if entity @s[tag=repick]",
            "comparison": "==",
            "compare_to": 1
        }
    },
