{
    "type": "origins:multiple",
    "default": {
        "type": "origins:action_on_callback",
        "entity_action_gained": {
            "type": "origins:and",
            "actions": [
                {
                    "type": "origins:execute_command",
                    "command": "scoreboard players set @s timer 15"
                },
                {
                    "type": "origins:execute_command",
                    "command": "scoreboard players set @s ready_timer 10"
                }
            ]
        },
        "execute_chosen_when_orb": true
    },
    "ready_timer": {
        "type": "origins:resource",
        "min": 0,
        "max": 10,
        "start_value": 10,
        "hud_render": {
            "should_render": false
        },
        "min_action": {
            "type": "origins:and",
            "actions": [
                {
                    "type": "origins:execute_command",
                    "command": "forceload add ~ ~ ~ ~"
                },
                {
                    "type": "origins:execute_command",
                    "command": "tag @a add playing"
                },
                {
                    "type": "origins:execute_command",
                    "command": "power grant @s manhunt:joining"
                },
                {
                    "type": "origins:execute_command",
                    "command": "power grant @a[tag=hunter] manhunt:countdown_effects"
                },
                {
                    "type": "origins:if_else_list",
                    "actions": [
                        {
                            "condition": {
                                "type": "origins:resource",
                                "resource": "manhunt:setup_armorstand_countdown",
                                "comparison": "==",
                                "compare_to": 30
                            },
                            "action": {
                                "type": "origins:execute_command",
                                "command": "execute as @a run resource set @s manhunt:countdown_effects_timer 600"
                            }
                        },
                        {
                            "condition": {
                                "type": "origins:resource",
                                "resource": "manhunt:setup_armorstand_countdown",
                                "comparison": "==",
                                "compare_to": 15
                            },
                            "action": {
                                "type": "origins:execute_command",
                                "command": "execute as @a run resource set @s manhunt:countdown_effects_timer 300"
                            }
                        },
                        {
                            "condition": {
                                "type": "origins:resource",
                                "resource": "manhunt:setup_armorstand_countdown",
                                "comparison": "==",
                                "compare_to": 10
                            },
                            "action": {
                                "type": "origins:execute_command",
                                "command": "execute as @a run resource set @s manhunt:countdown_effects_timer 200"
                            }
                        }
                    ]
                },
                {
                    "type": "origins:execute_command",
                    "command": "function manhunt:countdown"
                },
                {
                    "type": "origins:execute_command",
                    "command": "power remove @a manhunt:start"
                },
                {
                    "type": "origins:execute_command",
                    "command": "power remove @s manhunt:setup_armorstand_ready_timer_tick"
                }
            ]
        }
    },
    "ready_timer_tick": {
        "type": "origins:action_over_time",
        "entity_action": {
            "type": "origins:and",
            "actions": [
                {
                    "type": "origins:execute_command",
                    "command": "title @a actionbar [\"\",\"[\",{\"score\":{\"name\":\"@s\",\"objective\":\"ready_timer\"},\"bold\":true,\"color\":\"gold\"},\"] Game is starting! ⏱\"]"
                },
                {
                    "type": "origins:change_resource",
                    "resource": "manhunt:setup_armorstand_ready_timer",
                    "change": -1
                },
                {
                    "type": "origins:execute_command",
                    "command": "scoreboard players remove @s ready_timer 1"
                }
            ]
        },
        "interval": 20,
        "condition": {
            "type": "origins:command",
            "command": "execute unless entity @a[tag=!ready]",
            "comparison": "==",
            "compare_to": 1
        }
    },
    "ready_timer_reset": {
        "type": "origins:action_over_time",
        "rising_action": {
            "type": "origins:and",
            "actions": [
                {
                    "type": "origins:change_resource",
                    "resource": "manhunt:setup_armorstand_ready_timer",
                    "change": 10
                },
                {
                    "type": "origins:execute_command",
                    "command": "scoreboard players set @s ready_timer 10"
                }
            ]
        },
        "interval": 20,
        "condition": {
            "type": "origins:command",
            "command": "execute unless entity @a[tag=!ready]",
            "comparison": "==",
            "compare_to": 0
        }
    },
    "countdown": {
        "type": "origins:resource",
        "min": -1,
        "max": 30,
        "start_value": 15,
        "hud_render": {
            "should_render": false
        },
        "min_action": {
            "type": "origins:and",
            "actions": [
                {
                    "type": "origins:execute_command",
                    "command": "power remove @a[tag=hunter] manhunt:countdown_effects"
                },
                {
                    "type": "origins:execute_command",
                    "command": "tag @a remove ready"
                },
                {
                    "type": "origins:execute_command",
                    "command": "power remove @s manhunt:setup_armorstand"
                }
            ]
        }
    },
    "timer_tick": {
        "type": "origins:action_over_time",
        "entity_action": {
            "type": "origins:and",
            "actions": [
                {
                    "type": "origins:if_else_list",
                    "actions": [
                        {
                            "condition": {
                                "type": "origins:resource",
                                "resource": "manhunt:setup_armorstand_countdown",
                                "comparison": ">",
                                "compare_to": 5
                            },
                            "action": {
                                "type": "origins:and",
                                "actions": [
                                    {
                                        "type": "origins:execute_command",
                                        "command": "title @a title [\"\"]"
                                    },
                                    {
                                        "type": "origins:execute_command",
                                        "command": "title @a subtitle [\"\",\"[\",{\"score\":{\"name\":\"@s\",\"objective\":\"timer\"},\"bold\":true,\"color\":\"green\"},\"⏱]\"]"
                                    },
                                    {
                                        "type": "origins:execute_command",
                                        "command": "playsound minecraft:block.note_block.hat player @a ~ ~ ~ 3 1"
                                    }
                                ]
                            }
                        },
                        {
                            "condition": {
                                "type": "origins:and",
                                "conditions": [
                                    {
                                        "type": "origins:resource",
                                        "resource": "manhunt:setup_armorstand_countdown",
                                        "comparison": "<=",
                                        "compare_to": 5
                                    },
                                    {
                                        "type": "origins:resource",
                                        "resource": "manhunt:setup_armorstand_countdown",
                                        "comparison": ">",
                                        "compare_to": 2
                                    }
                                ]
                            },
                            "action": {
                                "type": "origins:and",
                                "actions": [
                                    {
                                        "type": "origins:execute_command",
                                        "command": "title @a title [\"\"]"
                                    },
                                    {
                                        "type": "origins:execute_command",
                                        "command": "title @a subtitle [\"\",\"[\",{\"score\":{\"name\":\"@s\",\"objective\":\"timer\"},\"bold\":true,\"color\":\"gold\"},\"⏱]\"]"
                                    },
                                    {
                                        "type": "origins:execute_command",
                                        "command": "playsound minecraft:block.note_block.hat player @a ~ ~ ~ 3 1.2"
                                    }
                                ]
                            }
                        },
                        {
                            "condition": {
                                "type": "origins:and",
                                "conditions": [
                                    {
                                        "type": "origins:resource",
                                        "resource": "manhunt:setup_armorstand_countdown",
                                        "comparison": "<=",
                                        "compare_to": 2
                                    },
                                    {
                                        "type": "origins:resource",
                                        "resource": "manhunt:setup_armorstand_countdown",
                                        "comparison": ">",
                                        "compare_to": 0
                                    }
                                ]
                            },
                            "action": {
                                "type": "origins:and",
                                "actions": [
                                    {
                                        "type": "origins:execute_command",
                                        "command": "title @a title [\"\"]"
                                    },
                                    {
                                        "type": "origins:execute_command",
                                        "command": "title @a subtitle [\"\",\"[\",{\"score\":{\"name\":\"@s\",\"objective\":\"timer\"},\"bold\":true,\"color\":\"dark_red\"},\"⏱]\"]"
                                    },
                                    {
                                        "type": "origins:execute_command",
                                        "command": "playsound minecraft:block.note_block.hat player @a ~ ~ ~ 3 1.5"
                                    }
                                ]
                            }
                        },
                        {
                            "condition": {
                                "type": "origins:resource",
                                "resource": "manhunt:setup_armorstand_countdown",
                                "comparison": "==",
                                "compare_to": 0
                            },
                            "action": {
                                "type": "origins:and",
                                "actions": [
                                    {
                                        "type": "origins:execute_command",
                                        "command": "title @a title [\"\"]"
                                    },
                                    {
                                        "type": "origins:execute_command",
                                        "command": "title @a subtitle {\"text\":\"GO!\",\"bold\":true,\"color\":\"yellow\"}"
                                    },
                                    {
                                        "type": "origins:execute_command",
                                        "command": "playsound minecraft:entity.wither.spawn player @a ~ ~ ~ 3 2"
                                    }
                                ]
                            }
                        }
                    ]
                },
                {
                    "type": "origins:execute_command",
                    "command": "scoreboard players remove @s timer 1"
                },
                {
                    "type": "origins:change_resource",
                    "resource": "manhunt:setup_armorstand_countdown",
                    "change": -1
                }
            ]
        },
        "interval": 20,
        "condition": {
            "type": "origins:resource",
            "resource": "manhunt:setup_armorstand_ready_timer",
            "comparison": "==",
            "compare_to": 0
        }
    }
}