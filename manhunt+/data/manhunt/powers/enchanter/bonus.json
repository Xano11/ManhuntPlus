{
    "name": "Grand Arcane: Vanish",
    "description": "You can teleport in front of you in the blink of an eye.",
    "type": "origins:multiple",
    "slash":{
        "type":"origins:entity_set"
    },
    "move": {
        "type": "origins:active_self",
        "entity_action": {
            "type": "origins:and",
            "actions": [
                {
                    "type": "origins:execute_command",
                    "command": "playsound minecraft:ui.toast.in player @a ~ ~ ~ 2 1.5",
                    "permission_level": 4
                },
                {
                    "type": "origins:swing_hand",
                    "hand": "MAIN_HAND"
                },
                {
                    "type": "origins:execute_command",
                    "command": "playsound minecraft:entity.camel.dash_ready player @a ~ ~ ~ 2 0.5",
                    "permission_level": 4
                },
                {
                    "type": "origins:spawn_entity",
                    "entity_type": "minecraft:armor_stand",
                    "tag": "{NoGravity:1b,Invulnerable:1b,Marker:1b,Invisible:1b,Tags:[\"vanish\"],Passengers:[{id:\"minecraft:item_display\",view_range:5f,Tags:[\"vanish\"],brightness:{sky:15,block:15},transformation:{left_rotation:[0.7f,0f,0f,0.71f],right_rotation:[0f,0f,0f,1f],translation:[0f,0f,0f],scale:[0.1f,0.1f,0.1f]},item:{id:\"minecraft:lapis_lazuli\",Count:1b,tag:{CustomModelData:1}}}]}",
                    "bientity_action":  {
                        "type": "origins:add_to_set",
                        "set": "manhunt:enchanter/vanish/activate_slash"
                    },
                    "entity_action": {
						"type": "origins:and",
						"actions": [
							{
								"type": "origins:execute_command",
								"command": "power grant @s manhunt:enchanter/vanish/slash",
								"permission_level": 4
							},
                            {
								"type": "origins:execute_command",
								"command": "tp @s ~ ~1 ~",
								"permission_level": 4
							},
							{
								"type": "origins:execute_command",
								"command": "execute on passengers run data modify entity @s Rotation set value [0f,0f]",
								"permission_level": 4
							},
							{
								"type": "origins:execute_command",
								"command": "execute on passengers run data modify entity @s interpolation_duration set value 20",
								"permission_level": 4
							},
							{
								"type": "origins:execute_command",
								"command": "execute on passengers run data modify entity @s transformation.scale set value [5f,5f,1.5f]",
								"permission_level": 4
							},
                            {
								"type": "origins:execute_command",
								"command": "execute on passengers run data modify entity @s start_interpolation set value -1",
								"permission_level": 4
							}
						]
					}
                }
            ]
        },
        "cooldown": 1200,
        "key": {
            "key": "key.origins.secondary_active",
            "continuous": false
        },
        "hud_render": {
            "should_render": true,
            "bar_index": 1,
            "sprite_location": "manhunt:textures/bars/enchanter/enchanter.png"
        }
    },
    "screen":{
        "type": "origins:inventory",
        "title": "Choose an Enchantment Buff!",
        "container_type": "hopper",
        "drop_on_death": false,
        "recoverable": false,
        "key": {
            "key": "key.hotbar.9"
        },
        "condition": {}
    },
    "duration": {
        "type": "origins:resource",
        "min": 0,
        "max": 40,
        "start_value": 40,
        "hud_render": {
            "should_render": false
        },
        "min_action": {
            "type": "origins:and",
            "actions": [
                {
                    "type": "origins:selector_action",
                    "selector": "@a[tag=hunter]",
                    "bientity_action": {
                        "type": "origins:and",
                        "actions": [
                            {
                                "type": "origins:target_action",
                                "action": {
                                    "type": "origins:execute_command",
                                    "command": "tag @s add slash_tp",
                                    "permission_level": 4
                                }
                            },
                            {
                                "type": "origins:actor_action",
                                "action":  {
                                    "type": "origins:execute_command",
                                    "command": "tp @p[tag=slash_tp] @s",
                                    "permission_level": 4
                                }
                            }
                        ]
                    },
                    "bientity_condition": {
                        "type": "origins:invert",
                        "condition": {
                            "type": "origins:in_set",
                            "set": "manhunt:enchanter/vanish/activate_slash"
                        }
                    }
                },
                {
                    "type": "origins:execute_command",
                    "command": "tag @a remove slash_tp",
                    "permission_level": 4
                },
                {
                    "type": "origins:execute_command",
                    "command": "particle minecraft:glow ~ ~1 ~ 0.3 0.3 0.3 30 100",
                    "permission_level": 4
                },
                {
                    "type": "origins:execute_command",
                    "command": "playsound minecraft:entity.enderman.teleport player @a ~ ~ ~ 2 1.3",
                    "permission_level": 4
                },                {
                    "type": "origins:execute_command",
                    "command": "execute on passengers run kill @s",
                    "permission_level": 4
                },
                {
                    "type": "origins:execute_command",
                    "command": "kill @s",
                    "permission_level": 4
                }
            ]
        }
    },
    "duration_down": {
        "type": "origins:action_over_time",
        "entity_action": {
            "type": "origins:change_resource",
            "resource": "manhunt:enchanter/vanish/slash_duration",
            "change": -1
        },
        "interval": 1
    },
    "hidden": false
}