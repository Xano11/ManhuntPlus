{
    "type": "origins:multiple",
    "resource": {
        "type": "origins:resource",
        "min": 0,
        "max": 80,
        "start_value": 80,
        "hud_render": {
            "should_render": false,
            "sprite_location": "origins:textures/gui/resource_bar.png",
            "bar_index": 2
        },
        "min_action": {
            "type": "origins:and",
            "actions": [
                {
                    "type": "origins:delay",
                    "ticks": 1,
                    "action": {
                        "type": "origins:and",
                        "actions": [
                            {
                                "type": "origins:execute_command",
                                "command": "kill @e[tag=bomb]"
                            },
                            {
                                "type": "origins:execute_command",
                                "command": "kill @e[tag=bomb_part]"
                            }
                        ]
                    }
                },
                {
                    "type": "origins:explode",
                    "power": 2,
                    "destruction_type": "break",
                    "damage_self": true,
                    "create_fire": false
                }
            ]
        }
    },
    "remove_resource": {
        "type": "origins:action_over_time",
        "entity_action": {
            "type": "origins:and",
            "actions": [
                {
                    "type": "origins:change_resource",
                    "resource": "manhunt:bomb_boom_resource",
                    "change": -10
                },
                {
                    "type": "origins:execute_command",
                    "command": "particle minecraft:campfire_signal_smoke ~ ~1 ~ 0 0 0 0.01 1"
                }
            ]
        },
        "interval": 10
    },
    "sound": {
        "type": "origins:action_on_callback",
        "entity_action_gained": {
            "type": "origins:execute_command",
            "command": "playsound minecraft:entity.generic.extinguish_fire player @a[distance=..32] ~ ~ ~ 1 0.5"
        }
    },
    "hidden": true
}