{
    "type": "origins:multiple",
    "resource": {
        "type": "origins:resource",
        "min": 0,
        "max": 300,
        "start_value": 300,
        "hud_render": {
            "should_render": false
        },
        "min_action": {
            "type": "origins:and",
            "actions": [
                {
                    "type": "origins:execute_command",
                    "command": "kill @s"
                }
            ]
        }
    },
    "remove_resource": {
        "type": "origins:action_over_time",
        "entity_action": {
            "type": "origins:and",
            "actions": [
                {
                    "type": "origins:change_resource",
                    "resource": "manhunt:smoke_bombing_resource",
                    "change": -1
                },
                {
                    "type": "origins:execute_command",
                    "command": "particle minecraft:campfire_signal_smoke ~ ~ ~ 3 2 3 0.01 10"
                }
            ]
        },
        "interval": 1
    },
    "apply_effect": {
        "type": "origins:action_over_time",
        "entity_action": {
            "type": "origins:and",
            "actions": [
                {
                    "type": "origins:area_of_effect",
                    "radius": 5,
                    "bientity_action": {
                        "type": "origins:target_action",
                        "action": {
                            "type": "origins:and",
                            "actions": [
                                {
                                    "type": "origins:change_resource",
                                    "resource": "manhunt:smoked_resource",
                                    "change": 30
                                },
                                {
                                    "type": "origins:execute_command",
                                    "command": "power grant @s manhunt:smoked",
                                    "permission_level": 4
                                }
                            ]
                        }
                    },
                    "bientity_condition": {
                        "type": "origins:and",
                        "conditions": [
                            {
                                "type": "origins:target_condition",
                                "condition": {
                                    "type": "origins:and",
                                    "conditions": [
                                        {
                                            "type": "origins:living"
                                        }
                                    ]
                                }
                            }
                        ]
                    }
                }
            ]
        },
        "interval": 1
    },
    "hidden": true
}