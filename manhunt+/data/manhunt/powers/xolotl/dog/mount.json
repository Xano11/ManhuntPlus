{
    "type": "origins:multiple",
    "summon": {
        "type": "origins:active_self",
        "entity_action": {
            "type": "origins:and",
            "actions": [
                {
                    "type": "origins:execute_command",
                    "command": "tag @s add riding_wolf",
                    "permission_level": 4
                },
                {
                    "type": "origins:execute_command",
                    "command": "playsound minecraft:entity.wolf.growl player @a ~ ~ ~ 3 0.8",
                    "permission_level": 4
                },
                {
                    "type": "origins:spawn_entity",
                    "entity_type": "minecraft:wolf",
                    "tag": "{AngerTime:150,PersistenceRequired:1b,CanPickUpLoot:0b,Age:0,CollarColor:1b,Tags:[\"xolotl_mount\"]}",
                    "entity_action": {
                        "type": "origins:and",
                        "actions": [
                            {
                                "type": "origins:execute_command",
                                "command": "scale delay set pehkui:base 0 @s",
                                "permission_level": 4
                            },
                            {
                                "type": "origins:execute_command",
                                "command": "scale set pehkui:base 3 @s",
                                "permission_level": 4
                            },
                            {
                                "type": "origins:execute_command",
                                "command": "power grant @s manhunt:xolotl/dog/doggo",
                                "permission_level": 4
                            }
                        ]
                    },
                    "bientity_action": {
                        "type": "origins:mount"
                    }
                },
                {
                    "type": "origins:execute_command",
                    "command": "playsound minecraft:item.nether_wart.plant player @a ~ ~ ~ 3 0.8"
                }
            ]
        },
        "cooldown": 1200,
        "key": {
            "key": "key.origins.secondary_active"
        },
        "hud_render": {
            "should_render": true,
            "bar_index": 7,
            "sprite_location": "origins:textures/gui/community/huang/resource_bar_02.png"
        }
    },
    "riding": {
        "type": "origins:action_over_time",
        "entity_action": {
            "type": "origins:riding_action",
            "action": {
                "type": "origins:and",
                "actions": [
                    {
                        "type": "origins:change_resource",
                        "resource": "manhunt:xolotl/dog/doggo_duration",
                        "change": -2
                    },
                    {
                        "type": "origins:execute_command",
                        "command": "data modify entity @s Rotation[0] set from entity @p[tag=riding_wolf] Rotation[0]"
                    },
                    {
                        "type": "origins:execute_command",
                        "command": "particle minecraft:dust_color_transition 0.7 0.5 0 1.5 0.8 0.55 0 ^ ^1 ^ 0.7 0.5 0.7 1 10"
                    },
                    {
                        "type": "origins:if_else",
                        "condition": {
                            "type": "origins:on_block"
                        },
                        "if_action": {
                            "type": "origins:add_velocity",
                            "z": 0.18,
                            "space": "local_horizontal_normalized"
                        }
                    }
                ]
            }
        },
        "interval": 1,
        "condition": {
            "type": "origins:riding",
            "bientity_condition": {
                "type": "origins:target_condition",
                "condition": {
                    "type": "origins:and",
                    "conditions": [
                        {
                            "type": "origins:entity_type",
                            "entity_type": "minecraft:wolf"
                        },
                        {
                            "type": "origins:power",
                            "power": "manhunt:xolotl/dog/doggo"
                        }
                    ]
                }
            }
        }
    },
    "name": "IM THE BIGGEST DAWG",
    "description": "Call upon forth the biggest of wolves to move at high speeds!",
    "badges": [
        {
            "type": "origins:tooltip",
            "sprite": "origins:textures/gui/badge/star.png",
            "text": "Summon a big wolf that grants great movement for 5.5s! Sneak to cancel early."
        },
        {
            "type": "origins:tooltip",
            "sprite": "origins:textures/gui/badge/toggle.png",
            "text": [
                "Press \"",
                {
                    "keybind": "key.origins.secondary_active"
                },
                "\" to ride the wolf!"
            ]
        },
        {
            "type": "origins:tooltip",
            "sprite": "manhunt:textures/icons/cooldown.png",
            "text": "Cooldown: 1m"
        }
    ]
}