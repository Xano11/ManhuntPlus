{
    "type": "origins:multiple",
    "default": {
        "type": "origins:action_on_callback",
        "entity_action_gained": {
            "type": "origins:and",
            "actions": [
                {
                    "type": "origins:execute_command",
                    "command": "scoreboard players set @s timer 15"
                },
                {
                    "type": "origins:execute_command",
                    "command": "scoreboard players set @s ready_timer 10"
                }
            ]
        },
        "execute_chosen_when_orb": true
    },
    "joining": {
        "type": "origins:action_over_time",
        "entity_action": {
            "type": "origins:selector_action",
            "selector": "@a",
            "bientity_action": {
                "type": "origins:target_action",
                "action": {
                    "type": "origins:execute_command",
                    "command": "power grant @s manhunt:start"
                }
            },
            "bientity_condition": {
                "type": "origins:target_condition",
                "condition": {
                    "type": "origins:power",
                    "power": "manhunt:start",
                    "inverted": true
                }
            }
        },
        "interval": 1,
        "condition": {
            "type": "origins:resource",
            "resource": "manhunt:hunt_ready_timer",
            "comparison": ">",
            "compare_to": 0
        }
    },
    "joining_after_timer": {
        "type": "origins:action_over_time",
        "entity_action": {
            "type": "origins:selector_action",
            "selector": "@a",
            "bientity_action": {
                "type": "origins:target_action",
                "action": {
                    "type": "origins:and",
                    "actions": [
                        {
                            "type": "origins:execute_command",
                            "command": "power remove @s manhunt:waiting"
                        },
                        {
                            "type": "origins:execute_command",
                            "command": "origin set @s origins:origin manhunt:spectator"
                        },
                        {
                            "type": "origins:execute_command",
                            "command": "tellraw @s \"A run is currently ongoing. Feel free to spectate!\""
                        },
                        {
                            "type": "origins:execute_command",
                            "command": "tag @s add spectating"
                        }
                    ]
                }
            },
            "bientity_condition": {
                "type": "origins:target_condition",
                "condition": {
                    "type": "origins:power",
                    "power": "manhunt:waiting"
                }
            }
        },
        "interval": 1,
        "condition": {
            "type": "origins:power",
            "power": "manhunt:joining"
        }
    },
    "daytime": {
        "type": "origins:action_over_time",
        "entity_action": {
            "type": "origins:execute_command",
            "command": "time set day"
        },
        "interval": 30,
        "condition": {
            "type": "origins:resource",
            "resource": "manhunt:hunt_ready_timer",
            "comparison": ">",
            "compare_to": 0
        }
    },
    "ban_start": {
        "type": "origins:resource",
        "min": 0,
        "max": 70,
        "start_value": 0,
        "max_action": {
            "type": "origins:and",
            "actions": [
                {
                    "type": "origins:selector_action",
                    "selector": "@a",
                    "bientity_action": {
                        "type": "origins:target_action",
                        "action": {
                            "type": "moborigins:show_floating_item",
                            "item_stack": {
                                "item": "minecraft:barrier"
                            }
                        }
                    }
                },
                {
                    "type": "origins:execute_command",
                    "command": "playsound minecraft:entity.experience_orb.pickup player @a ~ ~ ~ 10 0.5"
                }
            ]
        },
        "min_action": {
            "type": "origins:execute_command",
            "command": "function manhunt:setup/ban"
        }
    },
    "ban_down": {
        "type": "origins:action_over_time",
        "entity_action": {
            "type": "origins:change_resource",
            "resource": "manhunt:hunt_ban_start",
            "change": -10
        },
        "interval": 10,
        "condition": {
            "type": "origins:resource",
            "resource": "manhunt:hunt_ban_start",
            "comparison": ">",
            "compare_to": 0
        }
    },
    "ban_msg1": {
        "type": "origins:action_over_time",
        "rising_action": {
            "type": "origins:and",
            "actions": [
                {
                    "type": "origins:execute_command",
                    "command": "playsound minecraft:block.note_block.bit player @a ~ ~ ~ 10 0.5"
                },
                {
                    "type": "origins:execute_command",
                    "command": "tellraw @a \"TIME FOR BANS!1!11!!\""
                }
            ]
        },
        "interval": 1,
        "condition": {
            "type": "origins:resource",
            "resource": "manhunt:hunt_ban_start",
            "comparison": "==",
            "compare_to": 60
        }
    },
    "ban_msg2": {
        "type": "origins:action_over_time",
        "rising_action": {
            "type": "origins:and",
            "actions": [
                {
                    "type": "origins:execute_command",
                    "command": "playsound minecraft:block.note_block.bit player @a ~ ~ ~ 10 0.5"
                },
                {
                    "type": "origins:execute_command",
                    "command": "tag @r[tag=hunter] add ban_pick"
                },
                {
                    "type": "origins:execute_command",
                    "command": "tellraw @a \"All runners are given a ban vote...\""
                }
            ]
        },
        "interval": 1,
        "condition": {
            "type": "origins:resource",
            "resource": "manhunt:hunt_ban_start",
            "comparison": "==",
            "compare_to": 40
        }
    },
    "ban_msg3": {
        "type": "origins:action_over_time",
        "rising_action": {
            "type": "origins:and",
            "actions": [
                {
                    "type": "origins:execute_command",
                    "command": "playsound minecraft:block.note_block.bit player @a ~ ~ ~ 10 0.5"
                },
                {
                    "type": "origins:execute_command",
                    "command": "tellraw @a [\"\",\".. AND \",{\"selector\":\"@p[tag=ban_pick]\",\"bold\":true},\" CAN BAN A RUNNER!!!!\"]"
                }
            ]
        },
        "interval": 1,
        "condition": {
            "type": "origins:resource",
            "resource": "manhunt:hunt_ban_start",
            "comparison": "==",
            "compare_to": 20
        }
