    "ready_timer": {
        "type": "origins:resource",
        "min": 0,
        "max": 10,
        "start_value": 10,
        "hud_render": {
            "should_render": false
        },
        "min_action": {
            "type": "origins:and",
            "actions": [
                {
                    "type": "origins:execute_command",
                    "command": "forceload add ~ ~ ~ ~"
                },
                {
                    "type": "origins:execute_command",
                    "command": "tag @a add playing"
                },
                {
                    "type": "origins:execute_command",
                    "command": "tag @a[tag=hunter] add countdowned"
                },
                {
                    "type": "origins:execute_command",
                    "command": "power grant @s manhunt:joining"
                },
                {
                    "type": "origins:execute_command",
                    "command": "power remove @a manhunt:start_repick_key"
                },
                {
                    "type": "origins:execute_command",
                    "command": "power remove @a manhunt:start_repick"
                },
                {
                    "type": "origins:if_else",
                    "condition": {
                        "type": "origins:command",
                        "command": "execute if entity @s[tag=swift_enabled]",
                        "comparison": ">=",
                        "compare_to": 1
                    },
                    "if_action": {
                        "type": "origins:execute_command",
                        "command": "power grant @a[tag=runner] manhunt:swift"
                    }
                },
                {
                    "type": "origins:if_else",
                    "condition": {
                        "type": "origins:command",
                        "command": "execute if entity @s[tag=repick_enabled]",
                        "comparison": ">=",
                        "compare_to": 1
                    },
                    "if_action": {
                        "type": "origins:and",
                        "actions": [
                            {
                                "type": "origins:change_resource",
                                "resource": "manhunt:hunt_repick_timer",
                                "change": 400
                            },
                            {
                                "type": "origins:execute_command",
                                "command": "scoreboard players set @s repick_timer 20"
                            }
                        ]
                    },
                    "else_action": {
                        "type": "origins:and",
                        "actions": [
                            {
                                "type": "origins:execute_command",
                                "command": "power grant @a[tag=hunter] manhunt:countdown_effects"
                            },
                            {
                                "type": "origins:if_else",
                                "condition": {
                                    "type": "origins:command",
                                    "command": "execute if entity @a[tag=runner]",
                                    "comparison": "==",
                                    "compare_to": 1
                                },
                                "if_action": {
                                    "type": "origins:and",
                                    "actions": [
                                        {
                                            "type": "origins:change_resource",
                                            "resource": "manhunt:hunt_countdown",
                                            "change": 5
                                        },
                                        {
                                            "type": "origins:execute_command",
                                            "command": "scoreboard players add @s timer 5"
                                        }
                                    ]
                                }
                            },
                            {
                                "type": "origins:execute_command",
                                "command": "execute as @a[tag=hunter] run resource operation @s manhunt:countdown_effects_timer = @e[limit=1,sort=nearest,tag=setup] timer"
                            },
                            {
                                "type": "origins:execute_command",
                                "command": "function manhunt:countdown"
                            },
                            {
                                "type": "origins:execute_command",
                                "command": "power remove @a manhunt:start"
                            },
                            {
                                "type": "origins:execute_command",
                                "command": "power remove @s manhunt:hunt_ready_timer_tick"
                            }
                        ]
                    }
                },
                {
                    "type": "origins:if_else",
                    "condition": {
                        "type": "origins:command",
                        "command": "execute if entity @s[tag=convert_runners]",
                        "comparison": ">=",
                        "compare_to": 1
                    },
                    "if_action": {
                        "type": "origins:execute_command",
                        "command": "tag @a[tag=runner] add convert"
                    }
                }
            ]
        }
    },
    "ready_timer_tick": {
        "type": "origins:action_over_time",
        "entity_action": {
            "type": "origins:and",
            "actions": [
                {
                    "type": "origins:execute_command",
                    "command": "title @a actionbar [\"\",\"[\",{\"score\":{\"name\":\"@s\",\"objective\":\"ready_timer\"},\"bold\":true,\"color\":\"gold\"},\"] Game is starting! ⏱\"]"
                },
                {
                    "type": "origins:change_resource",
                    "resource": "manhunt:hunt_ready_timer",
                    "change": -1
                },
                {
                    "type": "origins:execute_command",
                    "command": "scoreboard players remove @s ready_timer 1"
                }
            ]
        },
        "interval": 20,
        "condition": {
            "type": "origins:and",
            "conditions": [
                {
                    "type": "origins:command",
                    "command": "execute unless entity @a[tag=!ready]",
                    "comparison": "==",
                    "compare_to": 1
                },
                {
                    "type": "origins:resource",
                    "resource": "manhunt:hunt_ready_timer",
                    "comparison": ">",
                    "compare_to": 0
                }
            ]
        }
    },
    "ready_timer_reset": {
        "type": "origins:action_over_time",
        "rising_action": {
            "type": "origins:and",
            "actions": [
                {
                    "type": "origins:change_resource",
                    "resource": "manhunt:hunt_ready_timer",
                    "change": 10
                },
                {
                    "type": "origins:execute_command",
                    "command": "scoreboard players set @s ready_timer 10"
                }
            ]
        },
        "interval": 1,
        "condition": {
            "type": "origins:command",
            "command": "execute unless entity @a[tag=!ready]",
            "comparison": "==",
            "compare_to": 0
        }
    },
