{
    "type": "origins:multiple",
    "timer": {
        "type": "origins:resource",
        "min": 0,
        "max": 600,
        "start_value": 300,
        "hud_render": {
            "should_render": false,
            "sprite_location": "origins:textures/gui/resource_bar.png",
            "bar_index": 2
        },
        "min_action": {
            "type": "origins:and",
            "actions": [
                {
                    "type": "origins:execute_command",
                    "command": "power remove @s manhunt:countdown_effects",
                    "permission_level": 4
                }
            ]
        }
    },
    "timer_tick": {
        "type": "origins:action_over_time",
        "entity_action": {
            "type": "origins:change_resource",
            "resource": "manhunt:countdown_effects_timer",
            "change": -10
        },
        "interval": 10
    },
    "no_move": {
        "type": "origins:modify_velocity",
        "modifier": {
            "value": -1,
            "operation": "multiply_total"
        },
        "axes": [
            "x",
            "y",
            "z"
        ]
    },
    "no_atk_speed": {
        "type": "origins:attribute",
        "modifier": {
            "name": "Decrease speed",
            "attribute": "minecraft:generic.attack_speed",
            "value": -3,
            "operation": "multiply_total"
        }
    },
    "no_elytra": {
        "type": "origins:prevent_elytra_flight"
    },
    "no_attack": {
        "type": "origins:modify_damage_dealt",
        "modifier": {
            "operation": "multiply_total",
            "value": -1
        }
    },
    "no_damage": {
        "type": "origins:modify_damage_taken",
        "modifier": {
            "name": "No damage",
            "operation": "multiply_total",
            "value": -1
        }
    },
    "prevent_item": {
        "type": "origins:prevent_item_use"
    },
    "prevent_block": {
        "type": "origins:prevent_block_use"
    },
    "no_break": {
        "type": "origins:modify_break_speed",
        "modifier": {
            "operation": "multiply_total",
            "value": -1
        }
    },
    "no_abilities": {
        "type": "eggolib:prevent_key_use",
        "keys": [
            "key.origins.primary_active",
            "key.origins.secondary_active"
        ]
    },
    "trigger_cooldowns":{
        "type": "origins:action_on_callback",
        "entity_action_gained": {
            "type": "origins:and",
            "actions": [
                {
                    "type": "origins:trigger_cooldown",
                    "power": "manhunt:imp/portal/portal_activate"
                },
                {
                    "type": "origins:trigger_cooldown",
                    "power": "manhunt:engineer/bacont/bacont_activate"
                }
            ]
        },
        "execute_chosen_when_orb": true
    },
    "hidden": true
}