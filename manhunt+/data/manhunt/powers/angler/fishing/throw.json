{
    "type": "origins:multiple",
    "rod": {
        "type": "origins:action_on_callback",
        "entity_action_gained": {
            "type": "origins:execute_command",
            "command": "give @s fishing_rod{Unbreakable:1,CustomModelData:1,Tags:[\"angler\"],display:{Name:'[{\"text\":\"Angler\\'s\",\"italic\":false,\"color\":\"dark_aqua\"},{\"text\":\" \"},{\"text\":\"Rod\",\"color\":\"aqua\"}]'},Enchantments:[{}]} 1"
        },
        "entity_action_respawned": {
            "type": "origins:execute_command",
            "command": "give @s fishing_rod{Unbreakable:1,CustomModelData:1,Tags:[\"angler\"],display:{Name:'[{\"text\":\"Angler\\'s\",\"italic\":false,\"color\":\"dark_aqua\"},{\"text\":\" \"},{\"text\":\"Rod\",\"color\":\"aqua\"}]'},Enchantments:[{}]} 1"
        },
        "entity_action_lost": {
            "type": "origins:execute_command",
            "command": "clear @s minecraft:fishing_rod"
        },
        "execute_chosen_when_orb": false
    },
    "cooldown": {
        "type": "origins:resource",
        "min": 0,
        "max": 700,
        "start_value": 0,
        "hud_render": {
            "should_render": true,
            "bar_index": 0,
            "sprite_location": "manhunt:textures/bars/angler/angler.png",
            "condition": {
                "type": "origins:resource",
                "resource": "manhunt:angler/fishing/throw_cooldown",
                "comparison": ">",
                "compare_to": 0
            }
        }
    },
    "cooldown_down": {
        "type": "origins:action_over_time",
        "entity_action": {
            "type": "origins:and",
            "actions": [
                {
                    "type": "origins:change_resource",
                    "resource": "manhunt:angler/fishing/throw_cooldown",
                    "change": -10
                }
            ]
        },
        "interval": 10,
        "condition": {
            "type": "origins:resource",
            "resource": "manhunt:angler/fishing/throw_fall",
            "comparison": "==",
            "compare_to": 0
        }
    },
    "use": {
        "type": "origins:active_self",
        "entity_action": {
            "type": "origins:and",
            "actions": [
                {
                    "type": "origins:delay",
                    "ticks": 1,
                    "action": {
                        "type": "origins:selector_action",
                        "selector": "@e[type=fishing_bobber,limit=1,sort=nearest]",
                        "bientity_action": {
                            "type": "origins:and",
                            "actions": [
                                {
                                    "type": "origins:mount"
                                },
                                {
                                    "type": "origins:target_action",
                                    "action": {
                                        "type": "origins:add_velocity",
                                        "z": 1,
                                        "space": "velocity"
                                    }
                                },
                                {
                                    "type": "origins:actor_action",
                                    "action": {
                                        "type": "origins:and",
                                        "actions": [
                                            {
                                                "type": "origins:change_resource",
                                                "resource": "manhunt:angler/fishing/throw_cooldown",
                                                "change": 700
                                            },
                                            {
                                                "type": "origins:change_resource",
                                                "resource": "manhunt:angler/fishing/throw_fall",
                                                "change": 1
                                            },
                                            {
                                                "type": "origins:execute_command",
                                                "command": "playsound minecraft:block.conduit.attack.target player @a ~ ~ ~ 2 2"
                                            }
                                        ]
                                    }
                                }
                            ]
                        },
                        "bientity_condition": {
                            "type": "origins:target_condition",
                            "condition": {
                                "type": "origins:riding",
                                "inverted": true
                            }
                        }
                    }
                }
            ]
        },
        "condition": {
            "type": "origins:and",
            "conditions": [
                {
                    "type": "apugli:key_pressed",
                    "key": {
                        "key": "key.origins.primary_active",
                        "continuous": true
                    }
                },
                {
                    "type": "origins:equipped_item",
                    "equipment_slot": "mainhand",
                    "item_condition": {
                        "type": "origins:nbt",
                        "nbt": "{Unbreakable:1,CustomModelData:1,Tags:[\"angler\"],display:{Name:'[{\"text\":\"Angler\\'s\",\"italic\":false,\"color\":\"dark_aqua\"},{\"text\":\" \"},{\"text\":\"Rod\",\"color\":\"aqua\"}]'},Enchantments:[{}]}"
                    }
                },
                {
                    "type": "origins:resource",
                    "resource": "manhunt:angler/fishing/throw_cooldown",
                    "comparison": "==",
                    "compare_to": 0
                }
            ]
        },
        "hud_render": {
            "should_render": false
        },
        "key": {
            "key": "key.use",
            "continuous": false
        },
        "cooldown": 1
    },
    "fall": {
        "type": "origins:resource",
        "min": 0,
        "max": 1,
        "start_value": 0,
        "hud_render": {
            "should_render": false
        }
    },
    "fall_immune":{
        "type": "origins:invulnerability",
        "damage_condition": {
            "type": "origins:in_tag",
            "tag": "minecraft:is_fall"
        },
        "condition": {
            "type": "origins:resource",
            "resource": "manhunt:angler/fishing/throw_fall",
            "comparison": "==",
            "compare_to": 1
        }
    },
    "fall_effect": {
        "type": "origins:modify_falling",
        "velocity": 0.02,
        "take_fall_damage": false,
        "condition": {
            "type": "origins:resource",
            "resource": "manhunt:angler/fishing/throw_fall",
            "comparison": "==",
            "compare_to": 1
        }
    },
    "stop_fall": {
        "type": "origins:action_over_time",
        "rising_action": {
            "type": "origins:change_resource",
            "resource": "manhunt:angler/fishing/throw_fall",
            "change": -1
        },
        "interval": 1,
        "condition": {
            "type": "origins:on_block"
        }
    },
    "kill_bobber": {
        "type": "origins:action_over_time",
        "rising_action": {
            "type": "origins:riding_action",
            "action": {
                "type": "origins:execute_command",
                "command": "kill @s"
            }
        },
        "interval": 1,
        "condition": {
            "type": "origins:riding",
            "bientity_condition": {
                "type": "origins:target_condition",
                "condition": {
                    "type": "origins:and",
                    "conditions": [
                        {
                            "type": "origins:entity_type",
                            "entity_type": "minecraft:fishing_bobber"
                        },
                        {
                            "type": "origins:nbt",
                            "nbt": "{OnGround:1b}"
                        }
                    ]
                }
            }
        }
    },
    "particles": {
        "type": "origins:action_over_time",
        "entity_action": {
            "type": "origins:riding_action",
            "action": {
                "type": "origins:execute_command",
                "command": "particle minecraft:firework ~ ~ ~ 0.3 0.1 0.3 0.01 1"
            }
        },
        "interval": 1,
        "condition": {
            "type": "origins:riding",
            "bientity_condition": {
                "type": "origins:target_condition",
                "condition": {
                    "type": "origins:entity_type",
                    "entity_type": "minecraft:fishing_bobber"
                }
            }
        }
    },
    "name": "Hook Surfing",
    "description": "You've learned all the tricks the fishing rod holds. The only thing holding you back is the brittle fishing rod's line.",
    "badges": [
        {
            "type": "origins:tooltip",
            "sprite": "origins:textures/gui/badge/star.png",
            "text": "(You will not take fall damage until you've hit the ground once.)"
        },
        {
            "type": "origins:tooltip",
            "sprite": "origins:textures/gui/badge/toggle.png",
            "text": [
                "Hold \"",
                {
                    "keybind": "key.origins.primary_active"
                },
                "\" and \"",
                {
                    "keybind": "key.use"
                },
                "\" to become one with the fishing bobber!\nRequires a fishing rod in hand."
            ]
        },
        {
            "type": "origins:tooltip",
            "sprite": "manhunt:textures/icons/cooldown.png",
            "text": "Cooldown: 35s"
        }
    ]
}