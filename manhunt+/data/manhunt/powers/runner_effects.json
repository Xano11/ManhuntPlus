{
    "type": "origins:multiple",
    "tag": {
        "type": "origins:action_on_callback",
        "entity_action_added": {
            "type": "origins:and",
            "actions": [
                {
                    "type": "origins:execute_command",
                    "command": "tag @s add runner"
                },
                {
                    "type": "origins:delay",
                    "ticks": 1,
                    "action": {
                        "type": "origins:execute_command",
                        "command": "execute store result score @s runners run execute if entity @a[tag=runner]"
                    }
                }
            ]
        },
        "entity_action_removed": {
            "type": "origins:and",
            "actions": [
                {
                    "type": "origins:execute_command",
                    "command": "tag @s remove runner"
                },
                {
                    "type": "origins:execute_command",
                    "command": "execute as @a[tag=runner] if score @s runners > @p runners run scoreboard players remove @s runners 1"
                },
                {
                    "type": "origins:execute_command",
                    "command": "scoreboard players reset @s runners"
                },
                {
                    "type": "origins:execute_command",
                    "command": "scoreboard players reset @s tracking"
                },
                {
                    "type": "origins:execute_command",
                    "command": "execute in minecraft:overworld kill @e[limit=1,sort=nearest,type=item,tag=tracking1]"
                },
                {
                    "type": "origins:execute_command",
                    "command": "execute in minecraft:the_nether run kill @e[limit=1,sort=nearest,type=item,tag=tracking2]"
                },
                {
                    "type": "origins:execute_command",
                    "command": "execute in minecraft:the_end run kill @e[limit=1,sort=nearest,type=item,tag=tracking3]"
                }
            ]
        },
        "entity_action_respawned": {
            "type": "origins:and",
            "actions": [
                {
                    "type": "origins:execute_command",
                    "command": "origin set @s origins:origin manhunt:spectator"
                },
                {
                    "type": "origins:execute_command",
                    "command": "origin set @s origins:runners manhunt:none"
                }
            ]
        },
        "execute_chosen_when_orb": true
    },
    "prevent_trace_render": {
        "type": "origins:prevent_entity_render",
        "bientity_condition": {
            "type": "origins:and",
            "conditions": [
                {
                    "type": "origins:target_condition",
                    "condition": {
                        "type": "origins:passenger",
                        "bientity_condition": {
                            "type": "origins:actor_condition",
                            "condition": {
                                "type": "origins:and",
                                "conditions": [
                                    {
                                        "type": "origins:entity_type",
                                        "entity_type": "minecraft:armor_stand"
                                    },
                                    {
                                        "type": "origins:power",
                                        "power": "manhunt:sanguis/sense/delete_trace"
                                    }
                                ]
                            }
                        },
                        "compare_to": 1,
                        "comparison": ">="
                    }
                },
                {
                    "type": "origins:actor_condition",
                    "condition": {
                        "type": "origins:power",
                        "power": "manhunt:sanguis/sense/intuition",
                        "inverted": true
                    }
                }
            ]
        }
    },
    "prevent_simulation_render": {
        "type": "origins:prevent_entity_render",
        "bientity_condition": {
            "type": "origins:and",
            "conditions": [
                {
                    "type": "origins:target_condition",
                    "condition": {
                        "type": "origins:passenger",
                        "bientity_condition": {
                            "type": "origins:actor_condition",
                            "condition": {
                                "type": "origins:and",
                                "conditions": [
                                    {
                                        "type": "origins:entity_type",
                                        "entity_type": "minecraft:armor_stand"
                                    },
                                    {
                                        "type": "origins:power",
                                        "power": "manhunt:engineer/bacont/render_wall"
                                    }
                                ]
                            }
                        },
                        "compare_to": 1,
                        "comparison": ">="
                    }
                },
                {
                    "type": "origins:actor_condition",
                    "condition": {
                        "type": "origins:power",
                        "power": "manhunt:engineer/bacont/simulated",
                        "inverted": true
                    }
                }
            ]
        }
    },
    "prevent_simulation_players": {
        "type": "origins:prevent_entity_render",
        "bientity_condition": {
            "type": "origins:and",
            "conditions": [
                {
                    "type": "origins:target_condition",
                    "condition": {
                        "type": "origins:and",
                        "conditions": [
                            {
                                "type": "origins:entity_type",
                                "entity_type": "minecraft:player"
                            },
                            {
                                "type": "origins:power",
                                "power": "manhunt:engineer/bacont/simulated"
                            }
                        ]
                    }
                },
                {
                    "type": "origins:actor_condition",
                    "condition": {
                        "type": "origins:power",
                        "power": "manhunt:engineer/bacont/simulated",
                        "inverted": true
                    }
                }
            ]
        }
    },
    "prevent_soul_pickup":{
        "type": "eggolib:prevent_item_pickup",
        "item_condition":  {
            "type": "origins:and",
            "conditions": [
                {
                    "type": "origins:nbt",
                    "nbt": "{CustomModelData:1}"
                },
                {
                    "type": "origins:ingredient",
                    "ingredient": {
                        "item": "minecraft:cyan_dye"
                    }
                }
            ]
        }
    },
    "compass2": {
        "type": "origins:action_over_time",
        "rising_action": {
            "type": "origins:and",
            "actions": [
                {
                    "type": "origins:execute_command",
                    "command": "function manhunt:nether"
                },
                {
                    "type": "origins:execute_command",
                    "command": "power remove @s manhunt:runner_effects_compass2"
                }
            ]
        },
        "interval": 5,
        "condition": {
            "type": "origins:and",
            "conditions": [
                {
                    "type": "origins:dimension",
                    "dimension": "minecraft:the_nether"
                }
            ]
        }
    },
    "compass3": {
