{
    "type": "origins:multiple",
    "duration": {
        "type": "origins:resource",
        "min": 0,
        "max": 1200,
        "start_value": 1200,
        "hud_render": {
            "should_render": false
        },
        "min_action": {
            "type": "origins:and",
            "actions": [
                {
                    "type": "origins:execute_command",
                    "command": "execute on vehicle run kill @s",
