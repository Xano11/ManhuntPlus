{
    "type": "origins:multiple",
    "double": {
        "type": "origins:resource",
        "min": 0,
        "max": 1,
        "start_value": 0,
        "hud_render": {
            "should_render": false
        }
    },
    "double_down": {
        "type": "origins:action_over_time",
        "rising_action": {
            "type": "origins:change_resource",
            "resource": "manhunt:chef/hops_double",
            "change": -1
        },
        "interval": 1,
        "condition": {
            "type": "origins:on_block"
        }
    },
    "use_time": {
        "type": "origins:resource",
        "min": 0,
        "max": 12,
        "start_value": 0,
        "hud_render": {
            "should_render": false
        }
    },
    "decay": {
        "type": "origins:action_over_time",
        "rising_action": {
            "type": "origins:change_resource",
            "resource": "manhunt:chef/hops_use_time",
            "change": -1
        },
        "interval": 1
    },
    "jump": {
        "type": "origins:modify_jump",
        "entity_action": {
            "type": "origins:and",
            "actions": [
                {
                    "type": "origins:change_resource",
                    "resource": "manhunt:chef/hops_double",
                    "change": 1
                },
                {
                    "type": "origins:change_resource",
                    "resource": "manhunt:chef/hops_use_time",
                    "change": 12,
                    "operation": "set"
                }
            ]
        }
    },
    "double_jump": {
        "type": "origins:active_self",
        "entity_action": {
            "type": "origins:and",
            "actions": [
                {
                    "type": "origins:change_resource",
                    "resource": "manhunt:chef/hops_double",
                    "change": -1
                },
                {
                    "type": "origins:change_resource",
                    "resource": "manhunt:chef/hops_use_time",
                    "change": 0,
                    "operation": "set"
                },
                {
                    "type": "origins:exhaust",
                    "amount": 0.4
                },
                {
                    "type": "origins:add_velocity",
                    "z": 0.25,
                    "y": 0.4,
                    "space": "local_horizontal_normalized"
                },
                {
                    "type": "origins:execute_command",
                    "command": "playsound minecraft:entity.frog.long_jump player @a ~ ~ ~ 1.3 0.5",
                    "permission_level": 4
                },
                {
                    "type": "origins:execute_command",
                    "command": "particle minecraft:cloud ~ ~ ~ 0.2 0.05 0.2 0.05 10 normal",
                    "permission_level": 4
                },
                {
                    "type": "origins:set_fall_distance",
                    "fall_distance": 0
                }
            ]
        },
        "condition": {
            "type": "origins:and",
            "conditions": [
                {
                    "type": "origins:fluid_height",
                    "fluid": "minecraft:water",
                    "comparison": "==",
                    "compare_to": 0
                },
                {
                    "type": "origins:fluid_height",
                    "fluid": "minecraft:lava",
                    "comparison": "==",
                    "compare_to": 0
                },
                {
                    "type": "origins:on_block",
                    "inverted": true
                },
                {
                    "type": "origins:resource",
                    "resource": "manhunt:chef/hops_use_time",
                    "comparison": ">",
                    "compare_to": 0
                },
                {
                    "type": "origins:resource",
                    "resource": "manhunt:chef/hops_double",
                    "comparison": ">",
                    "compare_to": 0
                }
            ]
        },
        "cooldown": 1,
        "hud_render": {
            "should_render": false
        },
        "key": {
            "key": "key.jump",
            "continuous": false
        }
    },
    "name": "Jamb√©e de Frog",
    "description": "Your background with frogs left you with superhuman abilities",
    "badges": [
        {
            "type": "origins:tooltip",
            "sprite": "origins:textures/gui/badge/star.png",
            "text": "(Can be used to negate fall damage when used near the ground.)\n(Also doesn't work in water/lava)"
        },
        {
            "type": "origins:tooltip",
            "sprite": "origins:textures/gui/badge/toggle.png",
            "text": [
                "Press \"",
                {
                    "keybind": "key.jump"
                },
                "\" in the air to do a second jump!"
            ]
        }
    ]
}