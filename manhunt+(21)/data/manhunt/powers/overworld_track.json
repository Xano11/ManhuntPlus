{
    "type": "origins:multiple",
    "following": {
        "type": "origins:action_over_time",
        "entity_action": {
            "type": "origins:execute_command",
            "command": "execute as @a[tag=runner] if score @s runners = @n[tag=tracking_overworld] tracking if entity @s[nbt={Dimension:\"minecraft:overworld\"}] run function manhunt:compass/follow_overworld"
        },
        "interval": 1
    },
    "update_itself": {
        "type": "origins:action_over_time",
        "entity_action": {
            "type": "origins:execute_command",
            "command": "execute as @a[tag=runner] if score @s runners = @n[tag=tracking_overworld] tracking if entity @s[nbt={Dimension:\"minecraft:overworld\"}] run function manhunt:compass/update_itself"
        },
        "interval": 1
    },
    "chunkload": {
        "type": "origins:action_over_time",
        "rising_action": {
            "type": "origins:execute_command",
            "command": "forceload add ~ ~"
        },
        "falling_action": {
            "type": "origins:execute_command",
            "command": "forceload remove ~ ~"
        },
        "interval": 1,
        "condition": {
            "type": "origins:command",
            "command": "execute at @a[tag=runner,nbt={Dimension:\"minecraft:overworld\"}] if score @s tracking = @p runners",
            "comparison": "==",
            "compare_to": 0
        }
    },
    "disabled": {
        "type": "origins:resource",
        "min": 0,
        "max": 1200,
        "start_value": 0
    },
    "track": {
        "type": "origins:action_over_time",
        "entity_action": {
            "type": "origins:selector_action",
            "selector": "@a[tag=hunter]",
            "bientity_action": {
                "type": "origins:target_action",
                "action": {
                    "type": "origins:modify_inventory",
                    "inventory_type": "inventory",
                    "item_condition": {
                        "type": "origins:custom_data",
                        "custom_data": "tracking_compass:1b"
                    },
                    "item_action": {
                        "type": "origins:modify",
                        "modifier": "manhunt:overworld_track"
                    }
                }
            },
            "bientity_condition": {
                "type": "origins:and",
                "conditions": [
                    {
                        "type": "origins:target_condition",
                        "condition": {
                            "type": "origins:dimension",
                            "dimension": "minecraft:overworld"
                        }
                    },
                    {
                        "type": "origins:actor_condition",
                        "condition": {
                            "type": "origins:resource",
                            "resource": "manhunt:overworld_track_disabled",
                            "comparison": "==",
                            "compare_to": 0
                        }
                    }
                ]
            }
        },
        "interval": 1
    },
    "hidden": true
}